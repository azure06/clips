{
  "name": "infiniti-clips",
  "version": "0.0.4",
  "author": "Infiniti",
  "homepage": "https://infiniticlips.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azure06/infiniti-clips.git"
  },
  "keywords": [
    "clipboard",
    "electron",
    "angular",
    "ionic",
    "universal",
    "windows",
    "mac"
  ],
  "bugs": {
    "url": "https://github.com/azure06/infiniti-clips/issues"
  },
  "main": "www/electron/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "ionicBuild": "ionic build --prod -- --base-href ./",
    "electronBuild": "tsc -p ./src/electron",
    "electronNoWait": "npm run electronBuild && electron .",
    "electron": "concurrently  \"SET BROWSER=none && npm run start\" \"npm run electronBuild\" \"wait-on http-get://localhost:4200 && electron .\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pack": "electron-builder --dir",
    "dist": "npm run ionicBuild && npm run electronBuild && electron-builder -p always",
    "publish": "electron-builder -p always"
  },
  "build": {
    "appId": "com.infiniti.clips",
    "productName": "Infiniti Clips",
    "publish": [
      {
        "provider": "github",
        "owner": "Infiniti",
        "repo": "infiniti-clips"
      }
    ],
    "directories": {
      "buildResources": "www"
    },
    "files": [
      "www/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "publish": [
        "github"
      ],
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/cdk": "^7.3.5",
    "@angular/common": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/fire": "^5.1.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.16",
    "@fortawesome/free-brands-svg-icons": "^5.8.0",
    "@fortawesome/free-solid-svg-icons": "^5.8.0",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^4.0.0",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "@ngx-share/button": "^7.1.2",
    "@ngx-share/buttons": "^7.1.2",
    "@ngx-share/core": "^7.1.2",
    "@types/quill": "^2.0.2",
    "@vitalets/google-translate-api": "^2.7.0",
    "auto-launch": "^5.0.5",
    "core-js": "^2.5.4",
    "electron-is-dev": "^1.0.1",
    "electron-log": "^3.0.5",
    "electron-updater": "^4.0.6",
    "firebase": "^5.9.1",
    "google-auth-library": "^3.1.0",
    "googleapis": "^37.2.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "quill": "^1.3.6",
    "rxjs": "~6.3.3",
    "universal-analytics": "^0.4.20",
    "uuid": "^3.3.2",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.12.3",
    "@angular-devkit/build-angular": "^0.800.2",
    "@angular-devkit/core": "~7.2.3",
    "@angular-devkit/schematics": "~7.2.3",
    "@angular/cli": "~7.2.3",
    "@angular/compiler": "~7.2.2",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/language-service": "~7.2.2",
    "@ionic/angular-toolkit": "~1.3.0",
    "@types/electron-is-dev": "^0.3.0",
    "@types/google.analytics": "0.0.39",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.0",
    "@types/uuid": "^3.4.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "electron": "^4.0.4",
    "electron-builder": "^20.43.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~8.0.0",
    "tslint": "~5.12.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.2",
    "wait-on": "^3.2.0"
  },
  "description": "Infiniti Clips"
}
